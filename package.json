{
  "name": "vue-component-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "sass": "^1.62.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-dts": "^2.3.0",
    "vue": "^3.2.47",
    "vue-tsc": "^1.4.2"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./components/componentA/ComponentA": {
      "import": {
        "types": "./dist/components/componentA/ComponentA.vue.d.ts",
        "default": "./dist/components/componentA/ComponentA.js"
      }
    },
    "./components/componentB/ComponentB": {
      "import": {
        "types": "./dist/components/componentB/ComponentB.vue.d.ts",
        "default": "./dist/components/componentB/ComponentB.js"
      }
    }
  }
}
